{
  "name": "myapp-rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-ts": "tsc && tsc-alias",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run serve-debug",
    "serve-debug": "nodemon --inspect dist/server/index.js",
    "serve": "node dist/server/index.js",
    "docker:build": "docker-compose up --build",
    "docker:up": "docker-compose up",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "NODE_ENV=test jest --runInBand --verbose --passWithNoTests",
    "coverage": "NODE_ENV=test jest --runInBand --verbose --collect-coverage --passWithNoTests",
    "create-db": "sequelize-cli db:create",
    "drop-db": "sequelize-cli db:drop",
    "apply-migrations": "sequelize-cli db:migrate",
    "revert-migrations": "sequelize-cli db:migrate:undo:all",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/sequelize": "^1.2.1",
    "admin-bro": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "express-validation": "^3.0.6",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.0",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sqlite3": "^5.0.1",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "sqlite3": "^5.0.1",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/async": "3.2.5",
    "@types/body-parser": "1.19.0",
    "@types/concurrently": "5.2.1",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.14",
    "@types/jest": "26.0.22",
    "@types/node": "^18.11.14",
    "@types/react": "^17.0.8",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/supertest": "2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.0.2",
    "debug": "^4.3.1",
    "eslint": "7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.1",
    "reflect-metadata": "^0.1.13",
    "sass": "1.32.6",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "tsc-alias": "^1.2.11",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**",
      "!**/tests/**",
      "!**/utils/**",
      "!**/**test**",
      "!**/middlewares/**"
    ],
    "testPathIgnorePatterns": [
      "/.imagine/",
      "<rootDir>/dist/"
    ]
  }
}
